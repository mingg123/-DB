<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btnConnect"
        android:layout_width="90dp"
        android:layout_height="87dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/returncall1" />


    <Button
        android:id="@+id/button1"
        android:layout_width="95dp"
        android:layout_height="91dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView3"
        android:background="@drawable/realcall"
        android:onClick="onButton1Clicked" />

    <Button
        android:id="@+id/button2"
        android:layout_width="95dp"
        android:layout_height="91dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="31dp"
        android:background="@drawable/realbus"
        android:onClick="onButton2Clicked"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="488dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="95dp"
        android:layout_height="91dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/reallocation"
        android:onClick="onButton3Clicked"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="488dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button1"
        android:layout_marginBottom="110dp"
        app:srcCompat="@drawable/imgbtn1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="71dp"
        android:layout_toStartOf="@+id/btnConnect"
        app:srcCompat="@drawable/imgbtn1" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView6"
        android:layout_marginEnd="56dp"
        app:srcCompat="@drawable/imgbtn1" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView7"
        android:layout_marginTop="82dp"
        app:srcCompat="@drawable/imgbtn1" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/button1"
        android:layout_marginTop="215dp"
        app:srcCompat="@drawable/imgbtn2" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:layout_alignStart="@+id/imageView7"
        android:layout_alignTop="@+id/imageView2"
        app:srcCompat="@drawable/imgbtn2" />

</RelativeLayout>